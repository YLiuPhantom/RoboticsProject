          Value: true
        elbow_pitch:
          Value: false
        elbow_yaw:
          Value: false
        gripper_finger1:
          Value: false
        gripper_finger2:
          Value: false
        shoulder_pitch:
          Value: false
        shoulder_roll:
          Value: false
        shoulder_yaw:
          Value: false
