        world:
          Value: true
        wrist_pitch:
          Value: false
        wrist_roll:
          Value: false
      Marker Scale: 0.15
      Name: TF
      Show Arrows: true
      Show Axes: true
      Show Names: true
      Tree:
        world:
          base_footprint:
            base_link:
              shoulder_roll:
                shoulder_pitch:
                  shoulder_yaw:
                    elbow_pitch:
                      elbow_yaw:
                        wrist_pitch:
